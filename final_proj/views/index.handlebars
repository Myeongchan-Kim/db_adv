<div class="main_container">
  <div class="toggleable control_box">
    <div class="attribute_wrapper">
      <div class="country_filter">
        <label for="nations_filter_chkbox">nations</label><input id="nations_filter_chkbox" type="checkbox" class="nations filter_checkbox" checked/>
        <label for="groups_filter_chkbox">groups</label><input id="groups_filter_chkbox" type="checkbox" class="groups filter_checkbox" />
      </div>
      {{#each indexObj}}
      <div class="index_div {{@key}}_index_div" data-attr-name="{{@key}}" data-group = "{{this.group}}" data-val="{{this.indicator_name}}">
        <label for="{{@key}}_attr" class="{{@key}}">{{@key}}</label>
        <input type=text class="attr_input" id="{{@key}}_attr" value="{{this.indicator_name}}" readonly />
        <button id="{{@key}}_attr_change_button" class="mdl-button mdl-button--raised mdl-js-button dialog-button">change</button>
        <dialog id="{{@key}}_search_dialog" class="mdl-dialog search_box {{@key}}_search_box">
          <h3 class="mdl-dialog__title">{{@key}} attribute</h4>
          <!-- <label for="{{@key}}_category" class="{{@key}} category">category</label>
          <select id="{{@key}}_category" name="category">
            {{#each ../categoryList}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
          </select> -->
          <input type="text" name="{{@key}}_search_input" id="{{@key}}_search_input" class="search_input"/>
          <button id="{{@key}}_search_button">Search</button>
          <div class="mdl-dialog__content">
            <div class="result_div">
            </div>
            <div class="mdl-dialog__actions close_part">
              <button type="button" id="{{@key}}_close_button" class="mdl-button close">Close</button>
            </div>
          </div>
        </dialog>
        <label for="{{@key}}_scale_type" class="{{@key}}">scale</label>
        <select id="{{@key}}_scale_type" name="scale_type">
          <option value="linear">linear</option>
          <option value="log">log</option>
          <option value="sqrt">sqrt</option>
        </select>
        <label for="{{@key}}_min" class="{{@key}} range min_range">min</label><input type=number class="range min_range" id="{{@key}}_min_input" value="{{this.min}}" />
        <label for="{{@key}}_max" class="{{@key}} range max_range">max</label><input type=number class="range max_range" id="{{@key}}_max_input" value="{{this.max}}" />
        <button name="{{@key}}_apply" class="{{@key}} apply" id="{{@key}}_apply_button">apply</button>
      </div>
      {{/each}}
      <button id="loadData" class="attr_set">Load Data</button>
    </div><!--attribute_wrapper-->
    <div class="year_wrapper">
      <label for="year">year</label>
      <input id="year_input" type="number" name="year" value="1960" />
      <button id="changeButton" class="year_set">change</button>
      <button id="playButton" class="play">Play!</button>
      <button id="stopButton" class="stop">Stop</button>
    </div><!--year_wrapper-->
  </div>
  <div id="screen">
  </div>
</div>
